#, kde-format
msgid "You do not have enough karma to access a shell or terminal emulation"
msgstr ""
"No teniu karma suficient per a accedir a un intèrpret d'ordres o una "
"emulació de terminal"

#: kateconsole.cpp:102 kateconsole.cpp:132 kateconsole.cpp:622
#, kde-format
msgid "Terminal"
msgstr "Terminal"

#: kateconsole.cpp:141
#, kde-format
msgctxt "@action"
msgid "&Pipe to Terminal"
msgstr "&Conduïx al terminal"

#: kateconsole.cpp:145
